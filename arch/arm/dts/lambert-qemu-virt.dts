/dts-v1/;
#include <dt-bindings/interrupt-controller/arm-gic.h>

/ {
	model = "Lambert Qemu Virtual Board";
	compatible = "axera,lambert-virt";
	#address-cells = <2>;
	#size-cells = <2>;
	interrupt-parent = <&gic>;

	chosen {
		stdout-path = "serial0:115200n8";
	};

	memory {
		reg = <0x4 0x00000000 0x0 0x80000000>;
		device_type = "memory";
	};

	aliases {
		serial0 = &serial0;
		serial1 = &serial1;
	};

	psci {
		compatible = "arm,psci-0.2";
		method = "smc";
	};

	gic: interrupt-controller@448000 {
		compatible = "arm,gic-400";
		#interrupt-cells = <3>;
		#address-cells = <2>;
		#size-cells = <2>;
		interrupt-controller;
		reg = <0x0 0x00449000 0x0 0x1000>,
			  <0x0 0x0044a000 0x0 0x2000>;
		interrupts = <GIC_PPI 9 IRQ_TYPE_LEVEL_HIGH>;
	};

	soc {
		compatible = "simple-bus";
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		serial0: uart@1068a000 {
			compatible = "snps,dw-apb-uart";
			reg = <0x0 0x1068a000 0x0 0x1000>;
			reg-shift = <2>;
			reg-io-width = <4>;
			interrupts = <GIC_SPI 41 IRQ_TYPE_LEVEL_HIGH>;
			clock-frequency = <192000000>; //192MHz
			u-boot,dm-pre-reloc;
			u-boot,dm-spl;
			status = "okay";
		};

		serial1: uart@1068b000 {
			compatible = "snps,dw-apb-uart";
			reg = <0x0 0x1068b000 0x0 0x1000>;
			reg-shift = <2>;
			reg-io-width = <4>;
			interrupts = <GIC_SPI 42 IRQ_TYPE_LEVEL_HIGH>;
			clock-frequency = <192000000>;
			u-boot,dm-pre-reloc;
			u-boot,dm-spl;
			status = "okay";
		};

		mmc0: mmc@10620000 {
			compatible = "ax,lambert-sdhci";
			reg = <0x0 0x10620000 0x0 0x1000>;
			interrupts = <0x0 0x9 0x4>;
			max-frequency = <200000000>;
			sdhci-caps-mask = <0xffffffff 0xffffffff>;
			sdhci-caps = <0x00000070 0x156ecc02>;
			bus-width = <8>;
			cap-mmc-highspeed;
			mmc-ddr-1_8v;
			mmc-hs200-1_8v;
			non-removable;
			no-sdio;
			no-sd;
			status = "okay";
		};

		mmc1: mmc@10621000 {
			compatible = "ax,lambert-sdhci";
			reg = <0x0 0x10621000 0x0 0x1000>;
			interrupts = <0x0 0xa 0x4>;
			max-frequency = <200000000>;
			bus-width = <8>;
			mmc-ddr-1_8v;
			mmc-hs200-1_8v;
			status = "disabled";
		};

		eth0: ethernet@60824000 {
			// clock-names = "phy_ref_clk", "apb_pclk";
			// clocks = <&clkc 17>, <&clkc 15>;
			compatible = "ax,lmt-dwmac-eqos";
			// interrupt-parent = <&intc>;
			interrupts = <0x0 0x1e 0x4>;
			reg = <0x0 0x60824000 0x0 0x4000>;
			phy-handle = <&phy1>;
			phy-mode = "gmii";
			// phy-reset-gpios = <&gpioctlr 43 GPIO_ACTIVE_LOW>;

			snps,en-tx-lpi-clockgating;
			snps,en-lpi;
			snps,write-requests = <2>;
			snps,read-requests = <16>;
			snps,burst-map = <0x7>;
			snps,txpbl = <8>;
			snps,rxpbl = <2>;

			dma-coherent;

			mdio {
				#address-cells = <0x1>;
				#size-cells = <0x0>;
				phy1: phy@1 {
					compatible = "ethernet-phy-ieee802.3-c22";
					device_type = "ethernet-phy";
					reg = <0x1>;
				};
			};
		};
	};
};